{
  "incremental": true,
  "indentWidth": 2,
  "lineWidth": 160,
  "rustfmt": {
    "imports_granularity": "Item"
  },
  "includes": [
    "**/*.{ts,tsx,js,jsx,json,md,html,scss,css,toml,rs}"
  ],
  "excludes": [
    "website/playground/build",
    "scripts/build-website",
    "**/node_modules",
    "**/dist",
    "**/target",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.62.2.wasm",
    "https://plugins.dprint.dev/json-0.14.1.wasm",
    "https://plugins.dprint.dev/markdown-0.12.2.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/prettier-0.5.1.exe-plugin@3b0f36f27222453e24a25786969e687a3bcef29f3df00c9d7246cba9f85956d3",
    "https://plugins.dprint.dev/rustfmt-0.5.3.exe-plugin@50d39f72a7be52d8a05d6099ed5fbbc1710b6acc57a5b389aabd0b9774ce6fde"
  ]
}
