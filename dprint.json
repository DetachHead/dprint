{
  "incremental": true,
  "indentWidth": 2,
  "lineWidth": 160,
  "rustfmt": {
    "imports_granularity": "Item"
  },
  "includes": [
    "**/*.{ts,tsx,js,jsx,json,md,html,scss,css,toml,rs}"
  ],
  "excludes": [
    "website/playground/build",
    "scripts/build-website",
    "**/node_modules",
    "**/dist",
    "**/target",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.62.0.wasm",
    "https://plugins.dprint.dev/json-0.14.0.wasm",
    "https://plugins.dprint.dev/markdown-0.12.1.wasm",
    "https://plugins.dprint.dev/toml-0.5.3.wasm",
    "https://plugins.dprint.dev/prettier-0.5.0.exe-plugin@41feaaad734f310a23d85d680fd4f858f95f83a06ecb336f533c8b25886b68c0",
    "https://plugins.dprint.dev/rustfmt-0.4.0.exe-plugin@c6bb223ef6e5e87580177f6461a0ab0554ac9ea6b54f78ea7ae8bf63b14f5bc2"
  ]
}
