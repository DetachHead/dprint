{
  "incremental": true,
  "indentWidth": 2,
  "lineWidth": 160,
  "rustfmt": {
    "imports_granularity": "Item"
  },
  "includes": [
    "**/*.{ts,tsx,js,jsx,json,md,html,scss,css,toml,rs}"
  ],
  "excludes": [
    "website/playground/build",
    "scripts/build-website",
    "**/node_modules",
    "**/dist",
    "**/target",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.63.0.wasm",
    "https://plugins.dprint.dev/json-0.14.1.wasm",
    "https://plugins.dprint.dev/markdown-0.12.2.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/prettier-0.6.0.exe-plugin@6cf9f835c092475f1a2ec04dc53bc890545c2afd1ea3d2a9b2eb7dd5027e1118",
    "https://plugins.dprint.dev/rustfmt-0.6.0.exe-plugin@8b65ed724170bd227e92a2f01d867e452ef7f26e78dc691999ffa37a276df27c"
  ]
}
