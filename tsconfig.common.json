{
    "compilerOptions": {
        "target": "ES2015",
        "module": "ES2015",
        "rootDir": "./src",
        "outDir": "./dist",
        "allowJs": true,
        "strict": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "downlevelIteration": true,
        "plugins": [{ "transform": "ts-nameof", "type": "raw" }],
        "importHelpers": true
    }
}
